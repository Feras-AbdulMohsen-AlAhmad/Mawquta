    {
      "name": "Hijri Calendar",
      "item": [
        {
          "name": "Get a Hijri Calendar for a Gregorian month",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/gToHCalendar/{{month}}/{{year}}",
              "host": ["{{base_url}}"],
              "path": ["gToHCalendar", "{{month}}", "{{year}}"]
            },
            "description": "Geocode city & country → returns coordinates and timezone info used by other endpoints."
          },
          "response": []
        },
        {
          "name": "Get a Gregorian a Calendar for a Hijri month",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/hToGCalendar/{{month}}/{{year}}",
              "host": ["{{base_url}}"],
              "path": ["hToGCalendar", "{{month}}", "{{year}}"]
            },
            "description": "Convert Gregorian date to Hijri (examples — see docs for full options)."
          },
          "response": []
        },
        {
          "name": "Convert a Gregorian date to a Hijri date",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/gToH/{{gregorianDate}}",
              "host": ["{{base_url}}"],
              "path": ["gToH", "{{gregorianDate}}"]
            },
            "description": "Convert Hijri date to Gregorian. (Endpoint naming & params may vary — check docs)."
          },
          "response": []
        },
        {
          "name": "Convert a Hijri date to a Gregorian date",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/hToG/{{hijriDate}}",
              "host": ["{{base_url}}"],
              "path": ["hToG", "{{hijriDate}}"]
            },
            "description": "Convert Hijri date to Gregorian. (Endpoint naming & params may vary — check docs)."
          },
          "response": []
        }
      ],
      "description": "Utility endpoints: geocoding/timezone and date conversions. (Some geocoding may rely on third-party services.)"
    },
