    {
      "name": "Geonames API",
      "item": [
        {
          "name": "Search Cities (Fulltext) - searchJSON",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{geonames_base_url}}/searchJSON?q={{geonames_query}}&maxRows={{geonames_maxRows}}&featureClass=P&username={{geonames_username}}&style=SHORT",
              "host": ["{{geonames_base_url}}"],
              "path": ["searchJSON"],
              "query": [
                {
                  "key": "q",
                  "value": "{{geonames_query}}"
                },
                {
                  "key": "maxRows",
                  "value": "{{geonames_maxRows}}"
                },
                {
                  "key": "featureClass",
                  "value": "P"
                },
                {
                  "key": "username",
                  "value": "{{geonames_username}}"
                },
                {
                  "key": "style",
                  "value": "SHORT"
                }
              ]
            },
            "description": "Autocomplete (fulltext) for populated places. Use featureClass=P to focus on cities/towns. Username is required."
          },
          "response": []
        },
        {
          "name": "Search Cities (Prefix) - name_startsWith",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{geonames_base_url}}/searchJSON?name_startsWith={{geonames_query}}&maxRows={{geonames_maxRows}}&featureClass=P&username={{geonames_username}}&style=SHORT",
              "host": ["{{geonames_base_url}}"],
              "path": ["searchJSON"],
              "query": [
                {
                  "key": "name_startsWith",
                  "value": "{{geonames_query}}"
                },
                {
                  "key": "maxRows",
                  "value": "{{geonames_maxRows}}"
                },
                {
                  "key": "featureClass",
                  "value": "P"
                },
                {
                  "key": "username",
                  "value": "{{geonames_username}}"
                },
                {
                  "key": "style",
                  "value": "SHORT"
                }
              ]
            },
            "description": "Autocomplete optimized for prefix typing. Returns populated places (featureClass=P). Username required."
          },
          "response": []
        },
        {
          "name": "Search Cities (Fulltext + Country Bias)",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{geonames_base_url}}/searchJSON?q={{geonames_query}}&maxRows={{geonames_maxRows}}&featureClass=P&countryBias={{geonames_countryBias}}&username={{geonames_username}}&style=SHORT",
              "host": ["{{geonames_base_url}}"],
              "path": ["searchJSON"],
              "query": [
                {
                  "key": "q",
                  "value": "{{geonames_query}}"
                },
                {
                  "key": "maxRows",
                  "value": "{{geonames_maxRows}}"
                },
                {
                  "key": "featureClass",
                  "value": "P"
                },
                {
                  "key": "countryBias",
                  "value": "{{geonames_countryBias}}"
                },
                {
                  "key": "username",
                  "value": "{{geonames_username}}"
                },
                {
                  "key": "style",
                  "value": "SHORT"
                }
              ]
            },
            "description": "Same searchJSON but biases results toward a specific country (ISO-3166-1 alpha-2), useful for Arabic users (e.g., SY, SA, EG)."
          },
          "response": []
        },
        {
          "name": "Reverse Geocode (Nearest City) - findNearbyPlaceNameJSON",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{geonames_base_url}}/findNearbyPlaceNameJSON?lat={{geonames_latitude}}&lng={{geonames_longitude}}&username={{geonames_username}}&style=SHORT",
              "host": ["{{geonames_base_url}}"],
              "path": ["findNearbyPlaceNameJSON"],
              "query": [
                {
                  "key": "lat",
                  "value": "{{geonames_latitude}}"
                },
                {
                  "key": "lng",
                  "value": "{{geonames_longitude}}"
                },
                {
                  "key": "username",
                  "value": "{{geonames_username}}"
                },
                {
                  "key": "style",
                  "value": "SHORT"
                }
              ]
            },
            "description": "Reverse geocoding: returns the closest populated place (feature class=P) for given lat/lng. Username required."
          },
          "response": []
        }
      ],
      "description": "GeoNames endpoints used for city autocomplete and reverse geocoding. Use https://secure.geonames.org and provide your GeoNames username."
