{
  "info": {
    "name": "AlAdhan API-MuslimSalat Collection",
    "_postman_id": "aladhan-muslimSalat-collection",
    "description": "Postman collection with main AlAdhan API endpoints (Prayer Times, Calendar, Qibla, Asma Al-Husna, Geo, OpenAPI docs). Base URL variable: {{base_url}} (default https://api.aladhan.com/v1).",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "variable": [
    {
      "key": "base_url",
      "value": "https://api.aladhan.com/v1",
      "type": "string"
    },
    { "key": "latitude", "value": "30.0444", "type": "string" },
    { "key": "longitude", "value": "31.2357", "type": "string" },
    { "key": "city", "value": "Cairo", "type": "string" },
    { "key": "country", "value": "Egypt", "type": "string" },
    { "key": "method", "value": "2", "type": "string" }
  ],
  "item": [
    {
      "name": "Prayer Times (Timings)",
      "item": [
        {
          "name": "Timings (by coordinates)",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/timings?latitude={{latitude}}&longitude={{longitude}}&method={{method}}",
              "host": ["{{base_url}}"],
              "path": ["timings"],
              "query": [
                { "key": "latitude", "value": "{{latitude}}" },
                { "key": "longitude", "value": "{{longitude}}" },
                { "key": "method", "value": "{{method}}" }
              ]
            },
            "description": "Get prayer times for today for specific coordinates. See docs for optional parameters (date, school, tune, midnightMode, etc.)."
          }
        },
        {
          "name": "TimingsByCity (city & country)",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/timingsByCity?city={{city}}&country={{country}}&method={{method}}",
              "path": ["timingsByCity"],
              "query": [
                { "key": "city", "value": "{{city}}" },
                { "key": "country", "value": "{{country}}" },
                { "key": "method", "value": "{{method}}" }
              ]
            },
            "description": "Get prayer times using city & country (API will geocode)."
          }
        },
        {
          "name": "TimingsByAddress (address string)",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/timingsByAddress?address=1600+Amphitheatre+Parkway+Mountain+View&method={{method}}",
              "path": ["timingsByAddress"],
              "query": [
                {
                  "key": "address",
                  "value": "1600 Amphitheatre Parkway Mountain View"
                },
                { "key": "method", "value": "{{method}}" }
              ]
            },
            "description": "Get prayer times by passing full address string. Useful when city/country are not sufficient."
          }
        },
        {
          "name": "Timings (by timestamp) - optional date",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/timings/{{timestamp}}?latitude={{latitude}}&longitude={{longitude}}&method={{method}}",
              "path": ["timings", "{{timestamp}}"],
              "query": [
                { "key": "latitude", "value": "{{latitude}}" },
                { "key": "longitude", "value": "{{longitude}}" },
                { "key": "method", "value": "{{method}}" }
              ]
            },
            "description": "Get timings for a specific unix timestamp (seconds). Replace {{timestamp}} with unix epoch seconds."
          }
        }
      ],
      "description": "Prayer Times endpoints: coordinates, city, address, timestamp. Supports many optional query params (method, school/asr, tune, midnightMode, adjustments)."
    },
    {
      "name": "Calendar (monthly / year)",
      "item": [
        {
          "name": "Calendar (by coordinates) - month",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/calendar?latitude={{latitude}}&longitude={{longitude}}&method={{method}}&month=1&year=2026",
              "path": ["calendar"],
              "query": [
                { "key": "latitude", "value": "{{latitude}}" },
                { "key": "longitude", "value": "{{longitude}}" },
                { "key": "method", "value": "{{method}}" },
                { "key": "month", "value": "1" },
                { "key": "year", "value": "2026" }
              ]
            },
            "description": "Get prayer times calendar for a specific month by coordinates. Useful to build printable calendars."
          }
        },
        {
          "name": "CalendarByCity (month)",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/calendarByCity?city={{city}}&country={{country}}&method={{method}}&month=1&year=2026",
              "path": ["calendarByCity"],
              "query": [
                { "key": "city", "value": "{{city}}" },
                { "key": "country", "value": "{{country}}" },
                { "key": "method", "value": "{{method}}" },
                { "key": "month", "value": "1" },
                { "key": "year", "value": "2026" }
              ]
            },
            "description": "Get monthly calendar by city & country."
          }
        },
        {
          "name": "CalendarByAddress (month)",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/calendarByAddress?address=1600+Amphitheatre+Parkway+Mountain+View&method={{method}}&month=1&year=2026",
              "path": ["calendarByAddress"],
              "query": [
                {
                  "key": "address",
                  "value": "1600 Amphitheatre Parkway Mountain View"
                },
                { "key": "method", "value": "{{method}}" },
                { "key": "month", "value": "1" },
                { "key": "year", "value": "2026" }
              ]
            },
            "description": "Get calendar by address string."
          }
        }
      ],
      "description": "Calendar endpoints provide monthly calendars (Gregorian with Hijri or vice versa)."
    },
    {
      "name": "Qibla",
      "item": [
        {
          "name": "Qibla (by coordinates)",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/qibla/{{latitude}}/{{longitude}}",
              "path": ["qibla", "{{latitude}}", "{{longitude}}"]
            },
            "description": "Calculate Qibla direction and angle for given coordinates. Alternative: /qibla?latitude=&longitude="
          }
        },
        {
          "name": "Qibla (query style)",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/qibla?latitude={{latitude}}&longitude={{longitude}}",
              "path": ["qibla"],
              "query": [
                { "key": "latitude", "value": "{{latitude}}" },
                { "key": "longitude", "value": "{{longitude}}" }
              ]
            },
            "description": "Alternative Qibla endpoint with query params."
          }
        }
      ],
      "description": "Qibla direction endpoints return angle & compass information."
    },
    {
      "name": "Asma Al-Husna (99 names)",
      "item": [
        {
          "name": "Get Asma Al-Husna (all)",
          "request": {
            "method": "GET",
            "header": [],
            "url": { "raw": "{{base_url}}/asma", "path": ["asma"] },
            "description": "Return the 99 names (Asma Al-Husna) in Arabic/English/transliteration. See docs for language & filters."
          }
        },
        {
          "name": "Get Asma (single by index)",
          "request": {
            "method": "GET",
            "header": [],
            "url": { "raw": "{{base_url}}/asma/1", "path": ["asma", "1"] },
            "description": "Get a single name by index (1..99)."
          }
        }
      ],
      "description": "Endpoints serving the 99 beautiful names of Allah (translations, transliterations, metadata)."
    },
    {
      "name": "Geo / Timezone / Convert (helper endpoints)",
      "item": [
        {
          "name": "Get Coordinates & Timezone by City (Geo)",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/geo?city={{city}}&country={{country}}",
              "path": ["geo"],
              "query": [
                { "key": "city", "value": "{{city}}" },
                { "key": "country", "value": "{{country}}" }
              ]
            },
            "description": "Geocode city & country → returns coordinates and timezone info used by other endpoints."
          }
        },
        {
          "name": "Convert Gregorian to Hijri",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/dateToHijri?date=2026-02-04",
              "path": ["dateToHijri"],
              "query": [{ "key": "date", "value": "2026-02-04" }]
            },
            "description": "Convert Gregorian date to Hijri (examples — see docs for full options)."
          }
        },
        {
          "name": "Convert Hijri to Gregorian",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/hijriToDate?hijri=1447-06-25",
              "path": ["hijriToDate"],
              "query": [{ "key": "hijri", "value": "1447-06-25" }]
            },
            "description": "Convert Hijri date to Gregorian. (Endpoint naming & params may vary — check docs)."
          }
        }
      ],
      "description": "Utility endpoints: geocoding/timezone and date conversions. (Some geocoding may rely on third-party services.)"
    },
    {
      "name": "Documentation / OpenAPI & misc",
      "item": [
        {
          "name": "OpenAPI - Prayer Times (yaml)",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "https://api.aladhan.com/v1/documentation/openapi/prayer-times/yaml",
              "host": ["https://api.aladhan.com"],
              "path": ["v1", "documentation", "openapi", "prayer-times", "yaml"]
            },
            "description": "Official OpenAPI (yaml) for Prayer Times (useful for auto-import)."
          }
        },
        {
          "name": "OpenAPI - Islamic Calendar (yaml)",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "https://api.aladhan.com/v1/documentation/openapi/islamic-calendar/yaml",
              "host": ["https://api.aladhan.com"],
              "path": [
                "v1",
                "documentation",
                "openapi",
                "islamic-calendar",
                "yaml"
              ]
            },
            "description": "OpenAPI for the Islamic Calendar endpoints."
          }
        },
        {
          "name": "OpenAPI - Qibla (yaml)",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "https://api.aladhan.com/v1/documentation/openapi/qibla/yaml",
              "host": ["https://api.aladhan.com"],
              "path": ["v1", "documentation", "openapi", "qibla", "yaml"]
            },
            "description": "OpenAPI for Qibla endpoints (useful for exact param names & examples)."
          }
        }
      ],
      "description": "Direct links to the API OpenAPI/YAML specs (good for automated import or verification)."
    }
  ]
}
